    "npm-run-all": "4.1.3",
    "protractor": "5.4.1",
    "rimraf": "2.6.2",
    "stylelint": "9.8.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-language-service": "0.9.9",
    "typescript": "3.1.6"
  }
}
